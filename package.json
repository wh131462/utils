{
  "name": "@eternalheart/utils",
  "version": "1.0.0",
  "description": "A js function utils library by EternalHeart.",
  "type": "module",
  "imports": {
    "#common/*": "./lib/common/*"
  },
  "bin": {
    "feature": "bin/feature.js"
  },

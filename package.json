{
  "name": "@eternalheart/utils",
  "version": "1.0.2",
  "description": "A js function utils library by EternalHeart.",
  "author": "EternalHeart",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/wh131462/utils"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "readme": "README.md",
  "imports": {
    "#node-lib/*": "./node-lib/*"
  },
  "bin": {
    "fc": "bin/feature.js"
  },
  "scripts": {
    "feature": "node bin/feature.js feature ",
    "build:types": "tsc --project tsconfig.json",
    "build:js": "node esbuild.config.js",
    "build": "run-s build:js build:types",
    "publish_": "npm run build && cd ./dist && npm publish",
    "publish:update": "npm version patch || true && npm run publish_",
    "unpublish_": "npm unpublish --force @eternalheart/utils"
  },
  "keywords": [
    "js",
    "eternalheart",
    "utils"
  ],
  "devDependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "esbuild": "^0.23.0",
    "glob": "^11.0.0",
    "inquirer": "^10.1.8",
    "npm-run-all": "^4.1.5",
    "ora": "^8.0.1",
    "tsc": "^2.0.4",
    "typescript": "^5.5.4"
  }
}
